            "pages": [
              "index",
              "quickstart",
              "setup"
            ]
          },
          {
            "group": "Tutorials",
            "pages": [
              "tutorial"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
