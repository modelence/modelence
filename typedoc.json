{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./server.ts", "./client.ts"],
  "exclude": ["**/README.md"],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/docs/api-reference/_generated",
  "excludePrivate": true,
  "excludeInternal": true,
  "pretty": true,
  "gitRevision": "main",
  "categorizeByGroup": true,
  "categoryOrder": ["Store", "Schema", "Module", "Methods", "*"],
  "cleanOutputDir": true,
  "entryPointStrategy": "expand",
  "groupOrder": ["Classes", "Variables", "React Hooks", "Functions", "Interfaces", "*"],
  "sort": ["alphabetical"],
  "hideGenerator": true,
  "readme": "none",
  "treatWarningsAsErrors": true,
  "validation": {
    "notExported": false
  },
  "intentionallyNotExported": ["InferDocumentType"],
  "preserveAnchorCasing": true,
  "useCodeBlocks": true,
  "publicPath": "/docs/api-reference"
} 