{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages",
  "entryPoints": ["packages/modelence", "packages/react-query", "packages/ai"],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/docs/api-reference/_generated",
  "excludePrivate": true,
  "excludeInternal": true,
  "pretty": true,
  "gitRevision": "main",
  "categorizeByGroup": true,
  "categoryOrder": ["Store", "Schema", "Module", "Methods", "React Query", "*"],
  "cleanOutputDir": true,
  "groupOrder": ["Classes", "Variables", "React Query", "Functions", "Interfaces", "*"],
  "sort": ["alphabetical"],
  "hideGenerator": true,
  "readme": "none",
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": false
  },
  "intentionallyNotExported": ["InferDocumentType"],
  "preserveAnchorCasing": true,
  "useCodeBlocks": true,
  "publicPath": "/docs/api-reference"
} 